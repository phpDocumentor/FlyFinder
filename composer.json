{
  "name": "phpdocumentor/flyfinder",
  "description": "Flysystem plugin to add file finding capabilities to the Filesystem entity",
  "keywords": ["flysystem", "phpdoc"],
  "homepage": "http://www.phpdoc.org",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Flyfinder\\": ["src/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flyfinder\\": ["tests/unit/"]
    }
  },
  "require": {
    "php":              ">=7.1",
    "league/flysystem": ">=1.0.41"
  },
  "minimum-stability":  "stable",
  "require-dev": {
    "mockery/mockery":  "~1",
    "league/flysystem-memory": "~1",
    "phpunit/phpunit": "~7"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
